<yandex>
    <default_on_cluster_name>test_cluster</default_on_cluster_name>
    <remote_servers>
        <test_cluster>
            <shard>
                <!-- both latest version -->
                <internal_replication>true</internal_replication>
                <replica>
                    <default_database>test</default_database>
                    <host>node1</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <default_database>test</default_database>
                    <host>node2</host>
                    <port>9000</port>
                </replica>
            </shard>
        </test_cluster>
    </remote_servers>
    <macros>
        <replica>node2</replica>
    </macros>
</yandex>